{
    "name": "shurik2k5/yii2-settings",
    "description": "Yii2 Database settings",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "config",
        "settings"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Aris Karageorgos",
            "email": "aris@phe.me"
        },
        {
            "name": "Alex Pogorelov",
            "email": "shurik2k5@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.13",
        "pheme/yii2-toggle-column": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "conflict": {
        "pheme/yii2-settings": "*"
    },
    "autoload": {
        "psr-4": {
            "pheme\\settings\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "shurik2k5\\settings\\tests\\": "./tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
